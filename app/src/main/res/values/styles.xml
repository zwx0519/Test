<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Home_Text">
        <item name="android:textSize">@dimen/dp_16</item>
        <item name="android:textColor">#000</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="channel" >
        <item name="android:layout_width">@dimen/dp_35</item>
        <item name="android:layout_height">@dimen/dp_35</item>
    </style>
    <style name="text" >
        <item name="android:layout_width">@dimen/dp_35</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="brand">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/dp_20</item>
    </style>
</resources>